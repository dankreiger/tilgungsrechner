{
  "name": "loans-web",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/loans/web",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/next:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/loans/web",
        "outputPath": "dist/apps/loans/web"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/loans/web"
        },
        "production": {}
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "loans-web:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "loans-web:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "loans-web:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nrwl/next:export",
      "options": {
        "buildTarget": "loans-web:build:production"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "apps/loans/web/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/loans/web/**/*.{ts,tsx,js,jsx}"
        ]
      }
    }
  },
  "tags": [
    "scope:loans",
    "platform:web",
    "type:app"
  ]
}
